<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <GameSpeed Name="Slow">
        <Gui Name="GameSpeedSlow">
            <Title>%GameSpeedSlowTitle</Title>
            <Description>%GameSpeedSlowDescription</Description>
        </Gui>
        <!-- EnumÃ©ration de tous les descriptors -->
        <Descriptors>CostSlow;GameSpeedSlow</Descriptors> <!--FoodRequiredSlow: now the growh factor is used-->
      
        <RegistryKeys>
          <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfIndustry/DustFundingGameSpeedFactor" Value="-0.25"/>
          <RegistryKey Path="/Game/VictoryConditions/TimeVictorySpeedFactor" Value="0.5"/>
          <RegistryKey Path="/Game/VictoryConditions/EconomicVictorySpeedFactor" Value="0.5"/>
          <RegistryKey Path="/Game/VictoryConditions/DiplomaticVictorySpeedFactor" Value="0.5"/>       
          <RegistryKey Path="/Game/RandomEventFactors/EventDurationCoef" Value="2"/>
          <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfForeignAffairs/StatusWaitingPeriodCoef" Value="2"/>
          <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfTheInterior/PopGrowthGameSpeedFactor" Value="0.25"/>
          <RegistryKey Path="/Game/Galaxy/StarSystemControl/TurnBeforeColonyStart" Value="45"/>
          <RegistryKey Path="/Game/Galaxy/StarSystemControl/TurnBeforeColonyMalus" Value="15"/>
          <RegistryKey Path="/Game/Galaxy/StarSystemControl/OwnershipGain" Value="0.02"/>
          <RegistryKey Path="/Game/Galaxy/StarSystemControl/MaxInvasionControlLoss" Value="5"/>
          <RegistryKey Path="/Game/Galaxy/StarSystemControl/InvasionControlLossCoef" Value="0.5"/>
          <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfTheInterior/FactionTraitOccasionalEffects/DurationCoef" Value="1.5"/>
          <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfTheInterior/PlanetLocustSteps/LimitsCoef" Value="1.5"/>
          <!--<RegistryKey Path="/Game/Empire/Agencies/DepartmentOfForeignAffairs/PlanetLocustSteps/Limits" Value="0;80;120"/> FORBIDDEN-->
        </RegistryKeys>
    </GameSpeed>

    <GameSpeed Name="Normal" Default="true">
        <Gui Name="GameSpeedNormal">
            <Title>%GameSpeedNormalTitle</Title>
            <Description>%GameSpeedNormalDescription</Description>
        </Gui>
      <Descriptors>GameSpeedNormal</Descriptors>
        <RegistryKeys>
            <RegistryKey Path="/Game/RandomEventFactors/EventDurationCoef" Value="1"/>
            <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfForeignAffairs/StatusWaitingPeriodCoef" Value="1"/>
        </RegistryKeys>
    </GameSpeed>

    <GameSpeed Name="Fast">
        <Gui Name="GameSpeedFast">
            <Title>%GameSpeedFastTitle</Title>
            <Description>%GameSpeedFastDescription</Description>
        </Gui>
        <Descriptors>CostFast;GameSpeedFast</Descriptors><!--FoodRequiredFast: now the growh factor is used-->

        <RegistryKeys>
          <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfIndustry/DustFundingGameSpeedFactor" Value="0.25"/>
          <RegistryKey Path="/Game/VictoryConditions/TimeVictorySpeedFactor" Value="-0.5"/>
          <RegistryKey Path="/Game/VictoryConditions/EconomicVictorySpeedFactor" Value="-0.5"/>
          <RegistryKey Path="/Game/VictoryConditions/DiplomaticVictorySpeedFactor" Value="-0.5"/>
          <RegistryKey Path="/Game/RandomEventFactors/EventDurationCoef" Value="0.5"/>
          <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfForeignAffairs/StatusWaitingPeriodCoef" Value="0.5"/>
          <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfTheInterior/PopGrowthGameSpeedFactor" Value="-0.25"/>
          <RegistryKey Path="/Game/Galaxy/StarSystemControl/TurnBeforeColonyStart" Value="15"/>
          <RegistryKey Path="/Game/Galaxy/StarSystemControl/TurnBeforeColonyMalus" Value="5"/>
          <RegistryKey Path="/Game/Galaxy/StarSystemControl/OwnershipGain" Value="0.08"/>
          <RegistryKey Path="/Game/Galaxy/StarSystemControl/MaxInvasionControlLoss" Value="20"/>
          <RegistryKey Path="/Game/Galaxy/StarSystemControl/InvasionControlLossCoef" Value="2"/>
          <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfTheInterior/FactionTraitOccasionalEffects/DurationCoef" Value="0.5"/>
          <RegistryKey Path="/Game/Empire/Agencies/DepartmentOfTheInterior/PlanetLocustSteps/LimitsCoef" Value="0.5"/>
          <!--<RegistryKey Path="/Game/Empire/Agencies/DepartmentOfForeignAffairs/PlanetLocustSteps/Limits" Value="0;20;30"/> FORBIDDEN-->
       </RegistryKeys>
    </GameSpeed>
    
</Datatable>